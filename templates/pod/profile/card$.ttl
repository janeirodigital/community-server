@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix interop: <http://www.w3.org/ns/solid/interop#> .
@prefix solid: <http://www.w3.org/ns/solid/terms#> .
@prefix ldp: <http://www.w3.org/ns/ldp#> .

<>
    a foaf:PersonalProfileDocument;
    foaf:maker <{{webId}}>;
    foaf:primaryTopic <{{webId}}>.

<{{webId}}>
    a foaf:Person ;
    a ldp:RDFSource ;
    foaf:name "{{name}}" ;
    a interop:Agent ;
    solid:oidcIssuer <{{oidcIssuer}}> ;
    interop:hasApplicationRegistrySet <./application#set> ;
    interop:hasDataRegistrySet <./data#set> ;
    interop:hasAccessGrantRegistrySet <./grant#set> ;
    interop:hasAccessReceiptRegistrySet <./receipt#set> ;
    interop:hasRemoteDataRegistrySet <./remote#set> .
