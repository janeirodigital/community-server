{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^0.0.0/components/context.jsonld",
  "@graph": [
    {
      "@id": "urn:solid-server:default:AclManager",
      "@type": "UrlBasedAclManager",
      "UrlBasedAclManager:_converter": {
        "@id": "urn:solid-server:default:RepresentationConverter"
      }
    },
    {
      "@id": "urn:solid-server:default:ShapeTreeAuxiliaryManager",
      "@type": "ShapeTreeAuxiliaryManager",
      "ShapeTreeAuxiliaryManager:_converter": {
        "@id": "urn:solid-server:default:RepresentationConverter"
      }
    },

    {
      "@id": "urn:solid-server:default:AclBasedAuthorizer",
      "@type": "WaterfallHandler",
      "WaterfallHandler:_handlers": [
        {
          "comment": "Auxiliary resources get converted by AuxiliaryAuthorizer and sent to WebAclAuthorizer. Other resources get rejected and get handled directly by WebAclAuthorizer.",
          "@type": "AuxiliaryAuthorizer",
          "AuxiliaryAuthorizer:_resourceAuthorizer": {
            "@id": "urn:solid-server:default:WebAclAuthorizer"
          },
          "AuxiliaryAuthorizer:_manager": {
            "@id": "urn:solid-server:default:AclManager"
          }
        },
        {
          "@id": "urn:solid-server:default:WebAclAuthorizer"
        }
      ]
    },

    {
      "@id": "urn:solid-server:default:WebAclAuthorizer",
      "@type": "WebAclAuthorizer",
      "WebAclAuthorizer:_aclManager": {
        "@id": "urn:solid-server:default:AclManager"
      },
      "WebAclAuthorizer:_resourceStore": {
        "@id": "urn:solid-server:default:ResourceStore"
      },
      "WebAclAuthorizer:_identifierStrategy": {
        "@id": "urn:solid-server:default:IdentifierStrategy"
      }
    }
  ]
}
